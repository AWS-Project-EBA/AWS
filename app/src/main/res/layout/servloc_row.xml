<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView_district"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:text="District: "
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_serviceloc"
        app:layout_constraintTop_toTopOf="@+id/imageView_serviceloc" />

    <TextView
        android:id="@+id/textView_serviceloc1"
        android:layout_width="234dp"
        android:layout_height="22dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_serviceloc"
        app:layout_constraintTop_toTopOf="@+id/imageView_serviceloc" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView_serviceloc"
        android:layout_width="96dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sloc" />

    <TextView
        android:id="@+id/textView_city"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:text="City: "
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_serviceloc2"
        app:layout_constraintTop_toTopOf="@+id/imageView_serviceloc2" />

    <TextView
        android:id="@+id/textView_serviceloc2"
        android:layout_width="232dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_serviceloc2"
        app:layout_constraintTop_toTopOf="@+id/imageView_serviceloc2" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView_serviceloc2"
        android:layout_width="96dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sloc" />

    <TextView
        android:id="@+id/textView_country"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:text="Country: "
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_serviceloc3"
        app:layout_constraintTop_toTopOf="@+id/imageView_serviceloc3" />

    <TextView
        android:id="@+id/textView_serviceloc3"
        android:layout_width="232dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView_serviceloc3"
        app:layout_constraintTop_toTopOf="@+id/imageView_serviceloc3" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView_serviceloc3"
        android:layout_width="96dp"
        android:layout_height="55dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sloc" />

</androidx.constraintlayout.widget.ConstraintLayout>